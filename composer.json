    "issues": "https://github.com/everstu/GPT3Tokenizer/issues",
    "source": "https://github.com/everstu/GPT3Tokenizer"
  },
  "scripts": {
    "test": "vendor/bin/phpunit -d memory_limit=-1 tests"
  },
  "authors": [
    {
      "name": "everstu",
      "email": "whtony2003@gmail.com"
    },
    {
      "name": "Jonas Duri",
      "email": "jonas.duri@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.3.3"
  },
  "autoload": {
    "psr-4": {
        "everstu\\Gpt3Tokenizer\\": "src/"
